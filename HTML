<html>
<head>
     </head> 
  <body>
    <button onclick="opengame()">TEXT ON THE BUTTON</button>
    <script>
      function opengame() {
        var win = window.open()
        var url = "URL HERE"
        var iframe = win.document.createElement('iframe')
        iframe.style.width = "100%";
        iframe.style.height = "100%";
        iframe.style.border = "none";
        iframe.src = url
        win.document.body.appendChild(iframe)
      }

    </script>
  </body>
</html>
